{"https://moirai2.github.io/schema/daemon/input":["$root","$days"],"https://moirai2.github.io/schema/daemon/bash":["data=\"$tmpdir/data.txt\"","esearch -db pubmed -query 'single-cell AND RNA-Sequencing AND mouse' -days $days|efetch -format docsum|xtract -pattern DocumentSummary -element Id SortFirstAuthor Title PubDate FullJournalName | perl $tmpdir/parser.pl $root > $data","sleep 2"],"https://moirai2.github.io/schema/daemon/script":{"https://moirai2.github.io/schema/daemon/script/name":"$tmpdir/parser.pl","https://moirai2.github.io/schema/daemon/script/code":["my $root=$ARGV[0];","while(<STDIN>){","chomp;","my ($pubmedid,$firstauthor,$title,$pubdate,$fulljournalname)=split(/\\t/);","print \"$root\\tpubmedid\\t$pubmedid\\n\";","print \"$pubmedid\\tfirstauthor\\t$firstauthor\\n\";","print \"$pubmedid\\ttitle\\t$title\\n\";","print \"$pubmedid\\tpubdate\\t$pubdate\\n\";","print \"$pubmedid\\tfulljournalname\\t$fulljournalname\\n\";","}"]},"https://moirai2.github.io/schema/daemon/import":"$data","https://moirai2.github.io/schema/daemon/singlethread":true}
