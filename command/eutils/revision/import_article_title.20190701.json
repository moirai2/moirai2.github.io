{"https://moirai2.github.io/schema/daemon/input":["$root","$query","$days"],"https://moirai2.github.io/schema/daemon/bash":["data=\"$tmpdir/data.txt\"","esearch -db pubmed -query \"$query\" -days $days|efetch -format docsum|xtract -pattern DocumentSummary -element Id SortFirstAuthor Title PubDate FullJournalName | perl $tmpdir/parser.pl $root > $data","id=(`cat $data | grep \"$root/pubmedid\" | cut -f3 | sort -u`)","sleep 2"],"https://moirai2.github.io/schema/daemon/script":{"https://moirai2.github.io/schema/daemon/script/name":"$tmpdir/parser.pl","https://moirai2.github.io/schema/daemon/script/code":["my $root=$ARGV[0];","while(<STDIN>){","chomp;","my ($pubmedid,$firstauthor,$title,$pubdate,$fulljournalname)=split(/\\t/);","print \"$root\\t$root/pubmedid\\t$pubmedid\\n\";","print \"$pubmedid\\t$root/firstauthor\\t$firstauthor\\n\";","print \"$pubmedid\\t$root/title\\t$title\\n\";","print \"$pubmedid\\t$root/pubdate\\t$pubdate\\n\";","print \"$pubmedid\\t$root/fulljournalname\\t$fulljournalname\\n\";","}"]},"https://moirai2.github.io/schema/daemon/import":"$data","https://moirai2.github.io/schema/daemon/outputs":"$id","https://moirai2.github.io/schema/daemon/singlethread":true}
